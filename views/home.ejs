<%- include("partials/header") -%>

<h1>Home</h1>
<p><%= pHome %></p>

<% posts.forEach(function(post) { %> 
    <% const newComposeTitle = post.composeTitle; %>
    <% const newPostContent = post.composeContent.substring(0, 100); %>
    <% const postId = post._id; %>
    <h3><%= newComposeTitle %></h3>
    <p><%= newPostContent %> ...<a href="/posts/<%= postId %>">Read more</a> </p>
<% }); %> <%-include("partials/footer") -%>


